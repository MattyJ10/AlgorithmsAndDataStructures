package BST;

import BST.BST.treeNode;

public class SecondLargestElementBST {

	int findSecondLargest(treeNode root) {
		treeNode temp = root; 
		int parentVal; 
		while(temp.right != null) {
			parentVal = temp.val; 
			temp = temp.right; 
		}
		if (temp.left != null) {
			temp = temp.left; 
			while(temp.right != null) {
				temp = temp.right; 
			}
			return temp.val;
		} else {
			return parentVal; 
		}
	}
	
}
