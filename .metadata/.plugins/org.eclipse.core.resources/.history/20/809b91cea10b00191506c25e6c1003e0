package Graph;

public class WeightedGraph {
	
	public class Edge {
		
		int weight; 
		GraphNode start;
		GraphNode end; 
		
		public Edge(int weight, GraphNode start, GraphNode end) {
			this.weight = weight; 
			this.start = start;
			this.end = end; 
		}
		
	}

